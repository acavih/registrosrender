{
  "name": "registrosrender",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:nuxt": "nuxt",
    "build:nuxt": "nuxt build",
    "start:nuxt": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "lintfix": "npm run lint:js -- --fix",
    "restoreToLocal": "gulp restoreToLocal",
    "dev": "concurrently --kill-others \"npm run dev:nuxt\" \"npm run dev --prefix registros-backend\"",
    "build:backend": "npm run build --workspace registros-backend",
    "build:frontend": "npm run generate",
    "build": "npm run build:backend && npm run build:frontend",
    "prod": "node registros-backend/dist/index.js",
    "start": "npm run prod"
  },
  "main": "registros-backend/dist/index.js",
  "workspaces": [
    "registros-backend"
  ],
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/lodash": "^4.14.182",
    "core-js": "^3.19.3",
    "dayjs": "^1.11.5",
    "gulp": "^4.0.2",
    "highcharts": "^10.2.0",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "nuxt-highcharts": "^2.0.4",
    "registros-types": "1.0.0",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1",
    "vuetify-dialog": "^2.0.17",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/express": "^4.17.13",
    "concurrently": "^7.3.0",
    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "nodemon": "^2.0.19",
    "npm-commands": "^1.1.0",
    "ts-node": "^10.9.1"
  }
}
